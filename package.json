{
  "name": "helper-reflinks",
  "description": "Async template helper for generating a list of markdown reference links.",
  "version": "4.0.0",
  "homepage": "https://github.com/helpers/helper-reflinks",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "helpers/helper-reflinks",
  "bugs": {
    "url": "https://github.com/helpers/helper-reflinks/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "arr-union": "^3.1.0",
    "reflinks": "^0.3.6"
  },
  "devDependencies": {
    "assemble-core": "^0.31.0",
    "engine-base": "^0.1.3",
    "engine-handlebars": "^0.8.2",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3"
  },
  "keywords": [
    "compile",
    "docs",
    "documentation",
    "format",
    "generate",
    "generator",
    "helper",
    "helpers",
    "link",
    "links",
    "markdown",
    "md",
    "ref",
    "reference",
    "reflink",
    "reflinks",
    "render",
    "template",
    "templates",
    "verb"
  ],
  "verb": {
    "data": {
      "alias": "reflinks"
    },
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "assemble",
      "generate",
      "handlebars",
      "lodash",
      "micromatch",
      "templates",
      "update",
      "verb",
      "verb-readme-generator"
    ],
    "related": {
      "list": [
        "handlebars-helpers",
        "helper-related",
        "template-helpers"
      ]
    },
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
